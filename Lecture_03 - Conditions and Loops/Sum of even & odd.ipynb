   "source": [
    "n = int(input())\n",
    "even=0\n",
    "odd=0\n",
    "while(n>0):\n",
    "    k=n%10\n",
    "    if(k%2==0):\n",
    "        even = even + k\n",
    "    else:\n",
    "        odd = odd + k\n",
    "    n=n//10\n",
    "\n",
    "print(even,\" \",odd)"
   ]
  }
